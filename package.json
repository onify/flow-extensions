{
  "name": "@onify/flow-extensions",
  "version": "4.0.0",
  "description": "Onify Flow extensions",
  "module": "index.js",
  "main": "dist/index.js",
  "sideEffects": false,
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha",
    "posttest": "npm run dist && eslint . --cache",
    "prepare": "npm run dist",
    "predist": "node -e \"fs.rmdirSync('./dist/src', {recursive: true});\"",
    "dist": "babel index.js -d dist && babel src -d dist/src && babel test/helpers/FlowScripts.js -d dist/src",
    "test:lcov": "c8 mocha -R dot && c8 report --reporter lcov && npm run posttest",
    "cov:html": "c8 mocha -R dot && c8 report --reporter html"
  },
  "keywords": [
    "onify",
    "flow",
    "bpmn",
    "bpmn-engine",
    "extensions"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/onify/flow-extensions.git"
  },
  "author": {
    "name": "Onify",
    "url": "https://github.com/onify"
  },
  "license": "MIT",
  "devDependencies": {
    "@aircall/expression-parser": "^1.0.4",
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "bpmn-elements-8-1": "npm:bpmn-elements@8.1",
    "bpmn-engine": "^15.1.2",
    "bpmn-engine-14": "npm:bpmn-engine@14",
    "bpmn-moddle": "^7.1.3",
    "c8": "^7.12.0",
    "camunda-bpmn-moddle": "^7.0.1",
    "chai": "^4.3.7",
    "chronokinesis": "^4.0.1",
    "debug": "^4.3.4",
    "eslint": "^8.32.0",
    "mocha": "^10.2.0",
    "mocha-cakes-2": "^3.3.0",
    "moddle-context-serializer": "^3.1.1"
  },
  "dependencies": {
    "cron-parser": "^4.7.1"
  },
  "files": [
    "src",
    "index.js",
    "dist/src",
    "dist/index.js"
  ],
  "c8": {
    "exclude": [
      "dist",
      "test",
      ".mocharc.js",
      "babel.config.js"
    ]
  }
}
